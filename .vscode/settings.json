
{
    "csharp.suppressDotnetRestoreNotification": true,
    "editor.renderWhitespace": "all",
    "editor.fontFamily": "Consolas, 'Microsoft YaHei Mono', monospace",
    "editor.renderControlCharacters": true,
    "omnisharp.projectLoadTimeout": 100,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
            "**/.vscode": true,
            "**/.vs": true,
            ".gitignore": true,
            "**/*.sln": true,
            "**/*.pssproj": true,
            "**/LICENSE": true
    },

    "[powershell]": {
            "files.encoding": "utf8bom",
            "files.autoGuessEncoding": true
    },

    "better-comments.tags": [
        {
          "tag": "WARN",
          "color": "#FFFF00",
          "strikethrough": false,
          "backgroundColor": "transparent"
        },
        {
          "tag": "FIXME",
          "color": "#FF2D00",
          "strikethrough": false,
          "backgroundColor": "transparent"
        },
        {
          "tag": "FAIL",
          "color": "#474747",
          "strikethrough": true,
          "backgroundColor": "transparent"
        },
        {
          "tag": "TODO:",
          "color": "#FFFF00",
          "strikethrough": false,
          "backgroundColor": "transparent"
        },
        {
          "tag": "NOTE:",
          "color": "#FFFFFF",
          "strikethrough": false,
          "backgroundColor": "transparent"
        }
    ],

